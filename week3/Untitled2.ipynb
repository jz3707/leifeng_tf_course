   "outputs": [],
   "source": [
    "import tensorflow as tf"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1.0770603, -0.17178775]\n"
     ]
    }
   ],
   "source": [
    "x = tf.Variable(8.0)\n",
    "m = tf.Variable(-1.0)\n",
    "y = tf.log(1 - tf.exp(m - tf.sin(x ** 2)))\n",
    "\n",
    "grad = tf.gradients(y, [x, m])\n",
    "\n",
    "with tf.Session() as sess:\n",
    "    sess.run(tf.global_variables_initializer())\n",
    "    print(sess.run(grad))"
   ]
  },
